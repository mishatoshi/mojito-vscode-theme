  "tokenColors": [
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#22774D"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "entity.name.variable",
        "variable.other punctuation.definition.variable"
      ],
      "settings": {
        "foreground": "#CCFFF6"
      }
    },
    {
      "name": "Constants and Other Variable",
      "scope": [
        "constant",
        "constant.other",
        "variable.other.constant",
        "variable.other.enummember",
        "support.constant",
        "variable.other.object",
        "support.other.variable",
        "variable.other.global",
        "variable.other.global punctuation.definition.variable"
      ],
      "settings": {
        "foreground": "#33E7FF"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#33E7FF"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "punctuation.definition.string.begin"
      ],
      "settings": {
        "foreground": "#FFED4D"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#3DA2F5"
      }
    },
    {
      "name": "Null",
      "scope": [
        "constant.language",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#3DA2F5"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.var.expr",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#CE6DDF",
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#CCFFF6"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF584D"
      }
    },
    {
      "name": "String Link",
      "scope": [
        "string.other.link"
      ],
      "settings": {
        "foreground": "#FFA726"
      }
    },
    {
      "name": "Keyword, Storage, Super, This, Self, Type",
      "scope": [
        "keyword",
        "keyword.other",
        "storage.type",
        "storage.modifier",
        "variable.language.this",
        "variable.language.this.php punctuation.definition.variable.php",
        "variable.language.super",
        "variable.language.self",
        "support.type",
        "constant.other.key",
        "keyword.other.type",
        "variable.language"
      ],
      "settings": {
        "foreground": "#3DA2F5"
      }
    },
    {
      "name": "Keyword: control, operator",
      "scope": [
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#CE6DDF"
      }
    },
    {
      "name": "Keyword Other Unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#33E7FF"
      }
    },
    {
      "name": "Meta Tag",
      "scope": [
        "meta.tag",
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#CCFFF6"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html",
        "keyword.operator.assignment.jsx",
        "keyword.operator.assignment.tsx"
      ],
      "settings": {
        "foreground": "#A7DD55"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FFB74D"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "#6AE26E"
      }
    },
    {
      "name": "Variable Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#EF5D8D"
      }
    },
    {
      "name": "Object Key",
      "scope": [
        "variable.other.property",
        "support.constant.property",
        "support.variable.property",
        "support.type.property-name",
        "variable.other.object.property",
        "punctuation.separator.key-value",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#CDDD3C"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.class",
