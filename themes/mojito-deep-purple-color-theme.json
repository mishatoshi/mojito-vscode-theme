  "tokenColors": [
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#70708F"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "entity.name.variable",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "constant",
        "constant.other",
        "variable.other.constant",
        "variable.other.enummember",
        "support.constant"
      ],
      "settings": {
        "foreground": "#4DEAFF"
      }
    },
    {
      "name": "Other Variable",
      "scope": [
        "support.other.variable",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#4DEAFF"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#FFED4D"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#F66055"
      }
    },
    {
      "name": "Null",
      "scope": [
        "constant.language",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#F66055"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.var.expr",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#CE6DDF",
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E57373"
      }
    },
    {
      "name": "String Link",
      "scope": [
        "string.other.link"
      ],
      "settings": {
        "foreground": "#FFA726"
      }
    },
    {
      "name": "Keyword, Storage, Super, This, Self",
      "scope": [
        "keyword",
        "keyword.other",
        "storage.type",
        "storage.modifier",
        "variable.language.this",
        "variable.language.this.php punctuation.definition.variable.php",
        "variable.language.super",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#55AEF6"
      }
    },
    {
      "name": "Keyword: control, operator",
      "scope": [
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#CE6DDF"
      }
    },
    {
      "name": "Keyword Other Unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#F66055"
      }
    },
    {
      "name": "Meta Tag",
      "scope": [
        "meta.tag",
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#B2E26A"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FF764D"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "#6AE26E"
      }
    },
    {
      "name": "Variable Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FFB74D"
      }
    },
    {
      "name": "Object Key",
      "scope": [
        "variable.other.property",
        "support.constant.property",
        "support.type.property-name",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#D2E052"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.module",
        "support.type",
        "support.class",
        "support.class.component",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#EF5D8D"
      }
    },
    {
      "name": "Entity Types",
