  "tokenColors": [
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#673AB7"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "entity.name.variable",
        "variable.language"
      ],
      "settings": {
        "foreground": "#EDE7F6"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "constant",
        "constant.other",
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#4DD0E1"
      }
    },
    {
      "name": "Other Variable",
      "scope": [
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#EDE7F6"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#4DD0E1"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.template"
      ],
      "settings": {
        "foreground": "#FFF176"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#FF8A65"
      }
    },
    {
      "name": "Null",
      "scope": [
        "constant.language",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#FF8A65"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.var.expr",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#FFF176",
      }
    },
    {
      "name": "Keyword Other Unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#4DD0E1"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#EDE7F6"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E57373"
      }
    },
    {
      "name": "String Link",
      "scope": [
        "string.other.link"
      ],
      "settings": {
        "foreground": "#FFA726"
      }
    },
    {
      "name": "Keyword, Storage, New, Super, This, Self",
      "scope": [
        "keyword",
        "keyword.other",
        "keyword.control",
        "keyword.operator.new",
        "punctuation.section.embedded",
        "storage.type",
        "storage.modifier",
        "variable.language.this",
        "variable.language.this.php punctuation.definition.variable.php",
        "variable.language.super",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#FF8A65"
      }
    },
    {
      "name": "Meta Tag",
      "scope": [
        "meta.tag"
      ],
      "settings": {
        "foreground": "#EDE7F6"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#AED581"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FFB74D"
      }
    },
    {
      "name": "Support Constant. Property-Value",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#EDE7F6"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "#AED581"
      }
    },
    {
      "name": "Variable Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FFB74D"
      }
    },
    {
      "name": "Object Key",
      "scope": [
        "variable.other.property",
        "support.constant.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#DCE775"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.module",
        "support.type",
        "support.class",
        "support.class.component",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#F48FB1"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type",
        "constant.other.key",
        "entity.name.type",
        "keyword.other.type"
      ],
      "settings": {
        "foreground": "#F48FB1"
      }
    },
    {
      "name": "Interface, Enum",
      "scope": [
        "interface",
        "entity.name.type.interface",
        "entity.name.type.enum"
