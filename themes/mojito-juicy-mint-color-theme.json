	"tokenColors": [
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#2EB881"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other",
				"variable.other.readwrite",
				"entity.name.variable",
				"variable.language"
			],
			"settings": {
				"foreground": "#E6E6E6"
			}
		},
		{
      "name": "Constants and Other Variable",
      "scope": [
        "constant",
        "constant.other",
        "variable.other.constant",
        "variable.other.enummember",
        "support.constant",
        "variable.other.object",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#4DEAFF"
      }
    },
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#4DEAFF"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.template",
				"punctuation.definition.string",
				"punctuation.definition.string.begin"
			],
			"settings": {
				"foreground": "#FFED4D"
			}
		},
		{
			"name": "Boolean",
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#F66055"
			}
		},
		{
			"name": "Null",
			"scope": [
				"constant.language",
				"constant.language.null"
			],
			"settings": {
				"foreground": "#F66055"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"meta.var.expr",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#CE6DDF",
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#E57373"
			}
		},
		{
			"name": "String Link",
			"scope": [
				"string.other.link"
			],
			"settings": {
				"foreground": "#42A5F5"
			}
		},
		{
			"name": "Keyword, Storage, Super, This, Self",
			"scope": [
				"keyword",
				"keyword.other",
				"storage.type",
				"storage.modifier",
				"variable.language.this",
				"variable.language.this.php punctuation.definition.variable.php",
				"variable.language.super",
				"variable.language.self"
			],
			"settings": {
				"foreground": "#55AEF6"
			}
		},
		{
			"name": "Keyword: control, operator",
			"scope": [
				"keyword.control",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#CE6DDF"
			}
		},
		{
			"name": "Keyword Other Unit",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#F66055"
			}
		},
		{
			"name": "Meta Tag",
			"scope": [
				"meta.tag",
				"text.html.derivative"
			],
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#B2E26A"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#FF764D"
			}
		},
		{
			"name": "Function, Special Method",
